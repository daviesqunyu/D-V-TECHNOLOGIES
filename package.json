{
  "name": "dv-technologies-react",
  "version": "2.0.0",
  "description": "D&V Technologies - Modern React Website",
  "main": "src/index.js",
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "serve": "node server.js",
    "build:analyze": "npm run build && npx webpack-bundle-analyzer build/static/js/*.js",
    "lint": "eslint src --ext .js,.jsx",
    "lint:fix": "eslint src --ext .js,.jsx --fix",
    "format": "prettier --write src/**/*.{js,jsx,json,css,md}",
    "audit-fix": "npm audit fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/daviesqunyu/D-V.git"
  },
  "keywords": ["react", "website", "business", "ai", "iot", "kenya"],
  "author": "D&V Technologies",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/daviesqunyu/D-V/issues"
  },
  "homepage": "https://daviesqunyu.github.io/D-V",
  "dependencies": {
    "express": "^4.17.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-scripts": "5.0.1",
    "styled-components": "^6.1.8",
    "framer-motion": "^10.18.0",
    "react-intersection-observer": "^9.5.3",
    "react-responsive": "^10.0.0"
  },
  "devDependencies": {
    "gh-pages": "^6.3.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
